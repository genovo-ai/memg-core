############################################
# MEMG example .env (minimal)
############################################

# Required API
GOOGLE_API_KEY=add_your_key_here
GEMINI_MODEL=gemini-2.0-flash

# Single source of truth for storage and port
BASE_MEMORY_PATH=$HOME/.local/share/memory_system
MEMORY_SYSTEM_MCP_PORT=8787

# Storage paths handled by Docker Compose

# Core runtime
QDRANT_COLLECTION=memories
MEMG_VECTOR_DIMENSION=768
EMBEDDING_DIMENSION_LEN=768
